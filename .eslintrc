{
  "extends": "airbnb",
  "rules": {
        "comma-dangle": 0,
        "semi": 0,
        "indent": [2, "tab"],
        "no-tabs": 0,
        "no-console": 0,
        "no-shadow": 0,
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
        "react/jsx-indent": 0,
        "linebreak-style": 0,
        "react/jsx-indent-props": 0,
        "jsx-a11y/img-has-alt": 0
    }
}
